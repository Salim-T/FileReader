      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_vLqWEEruEe2LdJ24AD6T2w" name="readReverseFile" specification="_vKJTIEruEe2LdJ24AD6T2w">
        <ownedParameter xmi:id="_vLqWEUruEe2LdJ24AD6T2w" name="fileString">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
        </ownedParameter>
        <language>java</language>
        <body>try {&#xD;
			File file = new File (fileString);	&#xD;
			if(file.exists()) {&#xD;
			BufferedReader br = new BufferedReader(new FileReader(file));&#xD;
			ArrayList&lt;String> A = new ArrayList&lt;>();&#xD;
			String line;&#xD;
			while ((line = br.readLine()) != null) {&#xD;
				A.add(line);&#xD;
			}&#xD;
			br.close();&#xD;
			Collections.reverse(A);			&#xD;
			for(String Lines : A) {&#xD;
				System.out.print(Lines + &quot;\n&quot;);&#xD;
			}&#xD;
		}&#xD;
		}&#xD;
		catch(Exception e) {&#xD;
			System.out.println(&quot;Erreur de lecture du fichier&quot;);&#xD;
		}&#xD;
	</body>
      </ownedBehavior>
