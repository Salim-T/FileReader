      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_vLpH8EruEe2LdJ24AD6T2w" name="readFile" specification="_vKFBsEruEe2LdJ24AD6T2w">
        <ownedParameter xmi:id="_vLpH8UruEe2LdJ24AD6T2w" name="fileString">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
        </ownedParameter>
        <language>java</language>
        <body>try {&#xD;
			File file = new File (fileString);	&#xD;
			if(file.exists()) {&#xD;
			BufferedReader br = new BufferedReader(new FileReader(file));&#xD;
			String line = br.readLine();&#xD;
			System.out.println(&quot;Voici le contenu du fichier à l'endroit&quot;);&#xD;
			while(line != null) {&#xD;
				System.out.println(line);&#xD;
				line = br.readLine();&#xD;
			}&#xD;
			br.close();&#xD;
			}&#xD;
		}&#xD;
		catch(Exception e) {&#xD;
			System.out.println(&quot;Erreur de lecture du fichier&quot;);&#xD;
		}&#xD;
	</body>
      </ownedBehavior>
      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_vLpvAEruEe2LdJ24AD6T2w" name="readPalindromeFile" specification="_vKG24EruEe2LdJ24AD6T2w">
